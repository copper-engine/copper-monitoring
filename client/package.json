{
  "name": "copper-monitoring-gui",
  "description": "Web GUI for copper engine",
  "version": "1.0.0",
  "author": "Vladyslav Gutsul <vladyslav.gutsul@scoop-software.cz>",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run compile",
    "deploy": "rm -rf ../server/static && mkdir -p ../server/static && cp -R dist/* ../server/static/",
    "deploy-win": "cd ../server & rmdir /s /q static & mkdir static && cd ../client && robocopy ./dist ../server/static /e & echo Deploymenet Complete",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "tslint src/**/*.ts",
    "serve": "http-server dist/ -g -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch"
  },
  "dependencies": {
    "@types/lodash": "^4.14.108",
    "axios": "~0.18.0",
    "chart.js": "^2.7.2",
    "lodash": "^4.17.10",
    "luxon": "^0.5.3",
    "mdi": "^2.2.43",
    "prismjs": "^1.14.0",
    "uiv": "^0.14.5",
    "vue": "~2.5.16",
    "vue-chartjs": "^3.3.1",
    "vue-chartkick": "^0.3.1",
    "vue-class-component": "^6.1.2",
    "vue-container": "^1.1.1",
    "vue-datetime": "^1.0.0-beta.3",
    "vue-google-charts": "0.0.3",
    "vue-moment": "^3.2.0",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-property-decorator": "~6.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "1.0.16",
    "vuex": "^3.0.1",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.0.58",
    "@types/sinon": "~4.0.0",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^7.2.6",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "~4.1.2",
    "compression-webpack-plugin": "~1.0.1",
    "copy-webpack-plugin": "~4.2.3",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "es6-promise": "~4.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "~0.10.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "json-loader": "~0.5.7",
    "karma": "~1.7.1",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.5",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.1",
    "karma-webpack": "^2.0.13",
    "lodash.merge": "^4.6.1",
    "minimist": "~1.2.0",
    "mocha": "~4.0.1",
    "ncp": "~2.0.0",
    "node-sass": "~4.7.2",
    "opn-cli": "~3.1.0",
    "postcss-loader": "^2.1.5",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "^0.9.6",
    "rimraf": "~2.6.2",
